{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.base.js"
      }
    }
  },
  "rules": {
    "no-console": 1,
    "arrow-parens": 0
  },
  "plugins": [
    "react",
    "import",
    "async-await"
  ]
}
